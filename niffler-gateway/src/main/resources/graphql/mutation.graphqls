type Mutation {
    createCategory(input: CreateCategoryInput!): Category!
    updateUser(input: UpdateUserInfoInput!): User!
    addSpend(generateSpend: SpendInput!): Spend!
    updateSpend(generateSpend: UpdateSpendInput!): Spend!
    deleteSpends(ids: [String]): [String!]!
    acceptInvitation(friendUsername: String!): User!
    declineInvitation(friendUsername: String!): User!
    addFriend(friendUsername: String!): User!
    removeFriend(friendUsername: String!): User!
}
